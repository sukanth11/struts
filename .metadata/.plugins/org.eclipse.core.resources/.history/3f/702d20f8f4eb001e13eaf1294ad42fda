package com.struts.action;

import com.struts.model.User;

public class LoginAction {
	
	private  User user;

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}
	
	public String execute() {
		if(user.getUsername().equals("sukanth"))
			return "success";
		return "index";
	}

}
